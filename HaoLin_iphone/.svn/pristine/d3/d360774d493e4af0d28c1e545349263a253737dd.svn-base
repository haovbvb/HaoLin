-(void)awakeFromNib
{
    self.Cancelbtn.backgroundColor =BGColor;
    [_Cancelbtn.layer setMasksToBounds:YES];
    [_Cancelbtn.layer setCornerRadius:5.0];
    _Cancelbtn.layer.borderWidth = 1;
    _Cancelbtn.layer.borderColor = [UIColor orangeColor].CGColor;
    
    self.Determinebtn.backgroundColor =BGColor;
    [_Determinebtn.layer setMasksToBounds:YES];
    [_Determinebtn.layer setCornerRadius:5.0];
    _Determinebtn.layer.borderWidth = 1;
    _Determinebtn.layer.borderColor = [UIColor orangeColor].CGColor;


}
